{
  "name": "nextui-vue-project",
  "type": "module",
  "version": "0.0.12",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "pnpm run -C playground dev",
    "dev:docs": "pnpm run -C docs dev",
    "build": "pnpm -r build",
    "clean": "rimraf -g node_modules */node_modules **/node_modules **/dist",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --cache --fix",
    "bumpp": "bumpp -r",
    "pub:view": "pnpm publish -r --dry-run --registry=https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.24.1",
    "bumpp": "^9.10.0",
    "eslint": "9.5.0",
    "pnpm": "^9.8.0",
    "rimraf": "^6.0.1"
  }
}
